{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "11.x"
  },
  "scripts": {
    "dev": "nodemon --exec babel-node --inspect ./src/index.js --ext js,gql | pino-pretty -t -c -f -l",
    "test": "jest --watch --runInBand --silent",
    "test-debugger": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "jest": "jest --ci --runInBand --silent --reporters=default --reporters=jest-junit",
    "build": "babel src --out-dir dist --copy-files",
    "start": "node dist/index.js",
    "knex": "babel-node node_modules/.bin/knex --knexfile ./src/db/config.js",
    "knex-migrate": "babel-node node_modules/.bin/knex-migrate --knexfile ./src/db/config.js",
    "heroku-knex": "knex --knexfile ./dist/db/config.js",
    "heroku-migrate": "knex-migrate --knexfile ./dist/db/config.js",
    "heroku-deploy": "cd ../ && git subtree push --prefix server heroku-server master || true",
    "lint": "eslint --ext .js --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "jest": {
    "globalSetup": "./tests/jest/globalSetup.js",
    "globalTeardown": "./tests/jest/globalTeardown.js",
    "reporters": [
      "default",
      "jest-junit"
    ],
    "verbose": true
  },
  "keywords": [],
  "author": "Sergei Anisimov",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "apollo-boost": "^0.3.1",
    "apollo-server": "^2.4.8",
    "bcrypt": "^3.0.5",
    "cross-fetch": "^3.0.1",
    "dotenv": "^7.0.0",
    "graphql": "^14.1.1",
    "graphql-middleware": "^3.0.2",
    "graphql-shield": "^5.3.1",
    "graphql-tools": "^4.0.4",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.16.3",
    "knex-migrate": "^1.7.1",
    "lodash": "^4.17.11",
    "objection": "^1.6.3",
    "objection-before-and-unique": "^0.5.0",
    "objection-guid": "^3.0.2",
    "password-validator": "^4.1.1",
    "pg": "^7.9.0",
    "pino": "^5.11.2",
    "pino-pretty": "^2.5.0",
    "schemaglue": "^4.0.2",
    "uuid": "^3.3.2",
    "validator": "^10.11.0",
    "faker": "^4.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/polyfill": "^7.4.0",
    "@babel/preset-env": "^7.3.4",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-root-import": "^6.1.0",
    "eslint": "^5.15.3",
    "eslint-plugin-node": "^8.0.1",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "prettier": "^1.16.4",
    "factory-girl": "^5.0.4",
    "factory-girl-objection-adapter": "^1.0.0",
    "get-port": "^4.2.0",
    "jest": "^24.5.0",
    "jest-junit": "^6.3.0",
    "nodemon": "^1.18.10"
  }
}
